{
    "deploy": {
        "packages": [
            {
                "files": [
                    "react-demo/build/**"
                ],
                "name": "React Demo",
                "targets": [
                    "neptun"
                ]
            }
        ],
        "targets": [
            {
                "agent": "${SSH_AUTH_SOCK}",
                "agentForward": true,
                "dir": "public_html/react-demo",
                "host": "neptun.g5r.eu",
                "mappings": [
                    {
                        "source": "react-demo/build",
                        "target": ""
                    }
                ],
                "name": "neptun",
                "port": 22,
                "type": "sftp",
                "user": "deploy"
            }
        ]
    },
    "jshint.enable": false,
    "prettier.eslintIntegration": true
}
